<!-- need to include buttons for home, dashboard, and login/logout button -->
<!-- Loop over each object in the post array -->
{{#each post as |post| }}
<div class="row mb-4 post">
  <div class="col-md-5">
    <div class="d-flex justify-content-between">
      <h2>{{post.title}}</h2>
      <p>Created by {{post.user.name}} on {{format_date post.date_created}}</p>
    </div>
    <div>
      <p>{{post.description}}</p>
    </div>
  </div>
</div>
{{/each}}